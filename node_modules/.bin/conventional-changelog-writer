C:/Users/MAKO/Desktop/Stage PFE/Amen/amenconnect/#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,/,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/conventional-changelog-writer/conventional-changelog-writer/cli.js" "$@"
fi
